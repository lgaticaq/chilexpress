{
  "name": "chilexpress",
  "version": "3.0.2",
  "description": "Check shipping status in chilexpress",
  "main": "src",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier-standard '{src,test}/**/*.js'",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "test": "echo \"no tests\""
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/chilexpress.git"
  },
  "keywords": [
    "chilexpress"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/chilexpress/issues"
  },
  "homepage": "https://github.com/lgaticaq/chilexpress#readme",
  "dependencies": {
    "chrome-aws-lambda": "^3.0.0",
    "debug": "4.2.0",
    "puppeteer": "^2.0.0",
    "puppeteer-core": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@eclass/eslint-config": "1.3.1",
    "@eclass/semantic-release-npm-github-config": "3.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/debug": "4.1.5",
    "@types/node": "12.19.1",
    "eslint": "7.12.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "26.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-standard": "4.0.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "prettier-standard": "16.4.1",
    "semantic-release": "17.2.1",
    "typescript": "3.9.7"
  },
  "eslintConfig": {
    "extends": "@eclass"
  },
  "eslintIgnore": [
    "coverage",
    "example.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "tonicExampleFilename": "example.js",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ]
  },
  "release": {
    "extends": "@eclass/semantic-release-npm-github-config"
  }
}
